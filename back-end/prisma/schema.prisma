generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TodoColumn {
  id         Int     @id @default(autoincrement())
  columnName String? @db.VarChar(255)
}

model TodoTask {
  id       Int     @id @default(autoincrement())
  taskName String? @db.VarChar(255)
  columnId Int
  deleted  Int     @default(0) @db.TinyInt
}
